---
interface Navlinks{
    href:string
    label:string
}

const links: Navlinks[] =[
    {href: "/", label: "Home"},
    {href: "/About", label: "About"},
    {href: "/Projects", label: "Projects"},
    {href: "/Contact", label: "Contact"}
]
---
<header>
    <nav>
        <ul>
            {links.map((link) =>(
                <li> 
                    <a href={link.href}>{link.label}</a>
                </li>
            ))}
        </ul>
    </nav>
</header>
